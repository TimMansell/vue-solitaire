{
  "name": "vue-solitaire",
  "version": "2.3.0",
  "private": true,
  "engines": {
    "node": ">=14.x"
  },
  "scripts": {
    "serve": "vite",
    "build": "run-p build:app build:functions",
    "build:app": "vite build",
    "build:functions": "netlify-lambda build src/functions --config ./webpack.functions.js",
    "preview": "run-p build:app preview:app",
    "preview:app": "vite preview",
    "test:unit": "jest",
    "test:e2e": "run-p -r serve test:e2e:cli",
    "test:e2e:gui": "run-p -r serve test:e2e:visual",
    "test:e2e:cli": "cypress run --headless --browser chrome --config ignoreTestFiles=\"**/*snapshot*\"",
    "test:e2e:snapshots": "run-p -r serve test:e2e:run:snapshots",
    "test:e2e:updatesnapshots": "run-p -r serve test:e2e:run:updatesnapshots",
    "test:e2e:visual": "cypress open --config ignoreTestFiles=\"**/*snapshot*\"",
    "test:e2e:run:snapshots": "cypress run --headless --config ignoreTestFiles=\"**/*spec*\"",
    "test:e2e:run:updatesnapshots": "cypress run --headless --env updateSnapshots=true --config ignoreTestFiles=\"**/*spec*\"",
    "lint": "run-p lint:js lint:css",
    "lint:js": "eslint {src,tests}/**/*.{vue,js} --fix",
    "lint:css": "stylelint src/**/*.{vue,scss} --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@graphql-tools/mock": "6.0.18",
    "@graphql-tools/schema": "6.0.18",
    "apollo-server-lambda": "2.25.1",
    "core-js": "3.14.0",
    "date-fns": "2.22.1",
    "dotenv": "8.2.0",
    "encoding": "0.1.13",
    "graphql": "15.5.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-tag": "^2.12.5",
    "hammerjs": "2.0.8",
    "lodash.shuffle": "4.2.0",
    "mongodb": "3.6.9",
    "mongodb-client-encryption": "1.2.5",
    "node-fetch": "2.6.1",
    "numeral": "2.0.6",
    "pause-me": "^1.2.0",
    "sanitize.css": "11.0.1",
    "semver": "^7.3.5",
    "socket.io-client": "4.1.3",
    "throttle-debounce": "3.0.1",
    "unique-names-generator": "^4.6.0",
    "uuid": "8.3.2",
    "vite": "2.5.10",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-vue2": "1.8.2",
    "vue": "2.6.12",
    "vue-router": "^3.5.2",
    "vue-scrollto": "2.20.0",
    "vue-toastification": "1.7.14",
    "vuejs-paginate": "2.1.0",
    "vuex": "3.6.2",
    "vuex-persistedstate": "3.2.0",
    "webpack-node-externals": "2.5.1",
    "xss": "^1.0.9"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.14.5",
    "@babel/preset-env": "^7.15.6",
    "@vue/test-utils": "1.2.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "^27.2.2",
    "cypress": "7.5.0",
    "cypress-commands": "1.1.0",
    "cypress-image-snapshot": "3.1.1",
    "cypress-wait-until": "^1.7.1",
    "eslint": "7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-vue": "7.11.1",
    "eslint-plugin-vue-scoped-css": "1.2.1",
    "jest": "27.2.2",
    "jest-matchmedia-mock": "1.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "10.5.4",
    "netlify-lambda": "2.0.9",
    "npm-run-all": "4.1.5",
    "sass": "1.42.1",
    "stylelint": "13.13.1",
    "stylelint-config-sass-guidelines": "7.1.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-webpack-plugin": "2.1.1",
    "timezone-mock": "^1.2.1",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "2.6.12",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ],
    "*.{scss,vue}": [
      "stylelint --fix",
      "git add"
    ]
  }
}
