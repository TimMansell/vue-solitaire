{
  "name": "vue-solitaire",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:functions": "netlify-lambda build src/functions",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e --headless --config ignoreTestFiles=\"**/*snapshot*\"",
    "test:e2e:gui": "vue-cli-service test:e2e --config ignoreTestFiles=\"**/*snapshot*\"",
    "test:e2e:snapshots": "vue-cli-service test:e2e --headless --config ignoreTestFiles=\"**/*spec*\"",
    "test:e2e:updatesnapshots": "vue-cli-service test:e2e --headless --env updateSnapshots=true --config ignoreTestFiles=\"**/*spec*\"",
    "lint": "run-p lint:js lint:css",
    "lint:js": "vue-cli-service lint --fix",
    "lint:css": "stylelint src/**/*.{vue,scss} --fix"
  },
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "encoding": "^0.1.13",
    "graphql": "^15.3.0",
    "hammerjs": "^2.0.8",
    "lodash.shuffle": "^4.2.0",
    "node-fetch": "^2.6.0",
    "register-service-worker": "^1.7.1",
    "sanitize.css": "^11.0.1",
    "vue": "^2.6.11",
    "vue-gtag": "^1.8.0",
    "vue-js-modal": "^1.3.34",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.4.4",
    "@vue/cli-plugin-e2e-cypress": "~4.4.4",
    "@vue/cli-plugin-eslint": "~4.4.4",
    "@vue/cli-plugin-unit-jest": "~4.4.4",
    "@vue/cli-plugin-vuex": "~4.4.4",
    "@vue/cli-service": "~4.4.4",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "1.0.3",
    "babel-eslint": "^10.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^10.2.11",
    "netlify-lambda": "^2.0.1",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "svgo": "^1.1.0",
    "svgo-loader": "^2.1.0",
    "vue-cli-plugin-svg-sprite": "~1.0.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{scss,vue}": [
      "stylelint --fix",
      "git add"
    ]
  }
}
